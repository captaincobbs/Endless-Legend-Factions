  <!-- Determines the chance for different types of starting planets
       Supposedly StarTypes is a thing, but I couldn't get it to work -->
	<GalaxyGeneratorWeightTable Name="MinorFactionKapaku_PlanetTypes">
    <Entry Tag="PlanetTypeLava"                 Weight="1"/>
  </GalaxyGeneratorWeightTable>
  <GalaxyGeneratorWeightTable Name="MinorFactionLords_PlanetTypes">
