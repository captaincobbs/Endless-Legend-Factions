<?xml version="1.0" encoding="utf-8"?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/PopulationModifiersTrait.xsd" >
  <!--  This is where the different population counts for each faction are stored
        A population count is incredibly resource intensive, so do as few as needed
        These are required to activate any population collection bonus that references them
        Currently there is a bug where these aren't being added to the empire simulation descriptor,
        causing anything that requires them to not be activated -->
  <!-- Allayi -->
  <PopulationModifiersTrait Name="PopulationModifiersAllayiCount" SubCategory="Count" >
    <Modifiers>
      <Modifier Class="ClassPopulationPlanet" >
        <SimulationDescriptorReference Name="ClassPopulationPlanetAffinityAllayiCount" />
      </Modifier>
      <Modifier Class="ClassPopulationStarSystem" >
        <SimulationDescriptorReference Name="ClassPopulationStarSystemAffinityAllayiCount" />
      </Modifier>
      <Modifier Class="ClassPopulationEmpire" >
        <SimulationDescriptorReference Name="ClassPopulationEmpireAffinityAllayiCount" />
      </Modifier>
    </Modifiers>
  </PopulationModifiersTrait>

