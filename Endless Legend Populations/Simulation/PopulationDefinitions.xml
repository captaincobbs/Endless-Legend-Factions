<?xml version="1.0" encoding="utf-8"?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/PopulationDefinition.xsd">
  <!-- Kapaku -->
  <PopulationDefinition AffinityName="AffinityKapaku" Standard="true"> <!-- "AffinityKapaku" is the definition for the Population, very important -->
    <DefaultPolitics> <!-- Politics = Base Political Party, Basescore is the starting strength of the political party, and I think trend is how fast it will automatically grow -->
      <Politics PoliticsReference="Politics01" BaseScore="1" BaseTrend="0"/>
    </DefaultPolitics>
    <!-- Political Traits-->
    <Trait Name="PopulationPoliticalTraitIndustrialist"/> <!-- This line -->
    <Trait Name="PopulationPoliticalTraitReligiousToMilitarist"/> <!-- This line -->
    <Trait Name="PopulationPoliticalTraitEcologist"/> <!-- And this line are used to define political traits -->
    <!-- Required stuff -->
    <Trait Name="PopulationModifiersKapakuCount"/> <!-- This line is used for the total population count -->
    <Trait Name="PopulationModifiersTraitKapaku"/> <!-- Defined in PopulationModifiersTraits -->
    <!-- Population Bonus traits (custom ones defined in PopulationModifiersTraits) -->
    <Trait Name="PopulationModifiersTraitSecondaryHappinessHot"/> <!-- +2 Happiness On Hot-->
    <Trait Name="PopulationModifiersTraitPrimaryIndustry"/> <!-- +1 Industry -->
    <!-- Collection Bonus Used -->
    <Trait Name="PopulationCollectionBonusTraitKapaku" />

    <AffinityMapping Name="AffinityMappingKapaku"/> <!-- Defined in SimulationDescriptors[FactionAffinityMapping] -->
    <GrowthBoostLuxuryOptions> <!-- Luxuries that can be used to boost population, goes for 1-16, game randomly chooses one each time based off of provided list here -->
      <Luxury>Luxury1</Luxury>
      <Luxury>Luxury2</Luxury>
      <Luxury>Luxury3</Luxury>
      <Luxury>Luxury4</Luxury>
      <Luxury>Luxury5</Luxury>
      <Luxury>Luxury6</Luxury>
      <Luxury>Luxury7</Luxury>
      <Luxury>Luxury8</Luxury>
    </GrowthBoostLuxuryOptions>
  </PopulationDefinition>

