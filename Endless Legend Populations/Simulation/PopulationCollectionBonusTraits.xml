  <!-- Mapping for the Population Collection Bonuses, defined in SimulationDescriptors[PopulationCollectionBonus]
       Threshold is the population required to reach that level, 10 for level 1, 20 for 2, and 50 for 3 are the defaults -->
  <!-- Kapaku -->
  <PopulationCollectionBonusTrait Name="PopulationCollectionBonusTraitKapaku" Cost="10" ShowInCustom="false">
    <CollectionBonus Threshold="10">
      <SimulationDescriptorReference Name="PopulationCollectionBonusKapaku1" />
      <SimulationDescriptorReference Name="PopulationCollectionBonusLevel1" />
    </CollectionBonus>

    <CollectionBonus Threshold="20">
      <SimulationDescriptorReference Name="PopulationCollectionBonusKapaku2" />
      <SimulationDescriptorReference Name="PopulationCollectionBonusLevel2" />
    </CollectionBonus>

    <CollectionBonus Threshold="50">
      <SimulationDescriptorReference Name="PopulationCollectionBonusKapaku3" />
      <SimulationDescriptorReference Name="PopulationCollectionBonusLevel3" />
    </CollectionBonus>
  </PopulationCollectionBonusTrait> 
    
