<?xml version="1.0" encoding="utf-8"?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Gui.GuiElement.xsd">
  <!-- Overrides are stored in EST_Localization_Locales
       Population Collection Bonuses are created in SimulationDescriptors[PopulationCollectionBonus]
       Overrides are used to make effects that don't automatically get a tooltip get a manual tooltip -->
  <!-- Kapaku -->
  <GuiElement Name="PopulationCollectionBonusKapaku1">
    <Title>%PopulationCollectionBonusKapaku1</Title>
  </GuiElement>
  <ExtendedGuiElement Name="PopulationCollectionBonusKapaku2">
    <Title>%PopulationCollectionBonusKapaku2</Title>
    <TooltipElement>
      <EffectOverride>%PopulationCollectionBonusKapaku2EffectOverride</EffectOverride>
    </TooltipElement>
  </ExtendedGuiElement>
  <ExtendedGuiElement Name="PopulationCollectionBonusKapaku3">
    <Title>%PopulationCollectionBonusKapaku3</Title>
    <TooltipElement>
      <EffectOverride>%PopulationCollectionBonusKapaku3EffectOverride</EffectOverride>
    </TooltipElement>
  </ExtendedGuiElement>
  
  <!-- Broken Lords -->
  <GuiElement Name="PopulationCollectionBonusLords1">
    <Title>%PopulationCollectionBonusLords1</Title>
  </GuiElement>
  <GuiElement Name="PopulationCollectionBonusLords2">
    <Title>%PopulationCollectionBonusLords2</Title>
  </GuiElement>
  <ExtendedGuiElement Name="PopulationCollectionBonusLords3">
    <Title>%PopulationCollectionBonusLords3</Title>
    <TooltipElement>
      <EffectOverride>%PopulationCollectionBonusLords3EffectOverride</EffectOverride>
    </TooltipElement>
  </ExtendedGuiElement>

  <!-- Forgotten -->
  <GuiElement Name="PopulationCollectionBonusForgotten1">
    <Title>%PopulationCollectionBonusForgotten1</Title>
  </GuiElement>
  <GuiElement Name="PopulationCollectionBonusForgotten2">
    <Title>%PopulationCollectionBonusForgotten2</Title>
  </GuiElement>
  <ExtendedGuiElement Name="PopulationCollectionBonusForgotten3">
    <Title>%PopulationCollectionBonusForgotten3</Title>
    <TooltipElement>
      <EffectOverride>%PopulationCollectionBonusForgotten3EffectOverride</EffectOverride>
    </TooltipElement>
  </ExtendedGuiElement>
</Datatable>