    <Description>%AffinityGameplayLordsDescription</Description>
    <Icons>
      <Icon Size="Small" Path="Gui/Lords/Small.png" />
      <Icon Size="Medium" Path="Gui/Lords/Medium.png" />
      <Icon Size="MediumInverted" Path="Bitmaps/Dynamic/Affinities/MediumInverted/AffinityHarmonyMediumInverted" />
      <Icon Size="Large" Path="Gui/Lords/Large.png" />
      <Icon Size="Mood" Path="Gui/Lords/Mood.png" />
