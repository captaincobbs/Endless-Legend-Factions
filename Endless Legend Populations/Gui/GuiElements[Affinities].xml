  <!-- Kapaku -->  
  <AffinityGuiElement Name="AffinityMappingKapaku">
    <Title>%AffinityGameplayKapakuTitle</Title>
    <Description>%AffinityGameplayKapakuDescription</Description>
    <Icons>
      <Icon Size="Small" Path="Gui/KapakuSmall.png" />                                                             <!-- Symbol that is displayed on system view screen 24*24-->
      <Icon Size="Medium" Path="Gui/KapakuMedium.png" />                                                           <!-- Symbol that the minor faction uses on galaxy screen 54*54-->
      <Icon Size="MediumInverted" Path="Bitmaps/Dynamic/Affinities/MediumInverted/AffinityHarmonyMediumInverted" /> <!-- No idea what it does -->
      <Icon Size="Large" Path="Gui/KapakuLarge.png" />                                                             <!-- Image used on population and minor faction diplomacy screen 540*270-->
      <Icon Size="Mood" Path="Gui/KapakuMood.png" />                                                               <!-- Image used when the faction is discovered 1324*712-->
    </Icons>
    <SymbolString>[custom02]</SymbolString>                                                                         <!-- Icon used for faction in interfaces -->
    <Color Red="255" Green="255" Blue="255" Alpha="255" />
  </AffinityGuiElement>

